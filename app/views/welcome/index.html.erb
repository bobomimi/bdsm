<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>






<% if user_signed_in? %>
                    <li class="dropdown">
                    <ul class="dropdown-menu">
                    <li><%= link_to 'edit', edit_user_registration_path %></li>
                    <li><%= link_to 'sign out', destroy_user_session_path, method: :delete %></li>
                    </ul>
                    </li>
                    <% else %>
                    <li style= "text-align:center; font-family:'Tangerine', serif;"><%= link_to 'sign up', new_user_registration_path %></li>
                    <li><%= link_to 'sign in', new_user_session_path %></li>
            <% end %>
            <% if admin_signed_in? %>
                    <li class="dropdown">
                    <ul class="dropdown-menu">
                    <li><%= link_to 'a edit', edit_admin_registration_path %></li>
                    <li><%= link_to 'a sign out', destroy_admin_session_path, method: :delete %></li>
                    <li><%= link_to 'admin', admin_path %></li>
                    </ul>
                    </li>
                    <% else %>
                    <li style= "text-align:center; font-family:'Tangerine', serif;"><%= link_to 'a sign up', new_admin_registration_path %></li>
                    <li><%= link_to 'a sign in', new_admin_session_path %></li>
            <% end %>